{
    "name": "n8n-nodes-weedb",
    "version": "1.0.72",
    "description": "",
    "keywords": [
        "n8n-community-node-package"
    ],
    "license": "MIT",
    "homepage": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/hkhoshraftar2/n8n-nodes-metisai"
    },
    "author": {
        "name": "Hossein Khoshraftar",
        "email": "hkhoshraftar@gmail.com"
    },
    "engines": {
        "node": ">=18.10",
        "pnpm": ">=9.1"
    },
    "packageManager": "pnpm@9.1.4",
    "main": "index.js",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "prepublishOnly": "pnpm build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 3,
        "credentials": [
            "dist/credentials/WeeDBConnection.credentials.js"
        ],
        "nodes": [
            "dist/nodes/WeeDBNode/WeeDBStringCreate.node.js",
            "dist/nodes/WeeDBNode/WeeDBKeyValueCreate.node.js",
            "dist/nodes/WeeDBNode/WeeDBKeyValueGet.node.js"
        ]
    },
    "devDependencies": {
        "@typescript-eslint/parser": "^7.15.0",
        "eslint": "^8.56.0",
        "eslint-plugin-n8n-nodes-base": "^1.16.1",
        "gulp": "^4.0.2",
        "prettier": "^3.3.2",
        "typescript": "^5.5.3"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "dependencies": {
        "axios": "^1.8.4"
    }
}
